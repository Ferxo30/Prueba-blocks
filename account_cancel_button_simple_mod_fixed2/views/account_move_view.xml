<odoo>
  <record id="view_move_form_force_cancel_inherit" model="ir.ui.view">
    <field name="name">account.move.form.force.cancel.inherit</field>
    <field name="model">account.move</field>
    <field name="inherit_id" ref="account.view_move_form"/>
    <field name="arch" type="xml">
      <xpath expr="//header" position="inside">
        <button name="action_force_cancel_and_draft"
                type="object"
                string="Forzar cancelar y volver a borrador"
                class="btn btn-secondary o_btn_force_cancel"
                groups="account.group_account_manager"
                attrs="{'invisible': ['|', ('state','=', 'draft'), ('move_type','=','entry')]}"
                help="Quita conciliaciones (si las hay), cancela el asiento y regresa a estado borrador."/>
      </xpath>
    </field>
  </record>
</odoo>
