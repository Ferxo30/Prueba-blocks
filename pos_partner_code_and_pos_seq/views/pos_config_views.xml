
<odoo>
  <record id="view_pos_config_form_internal_seq" model="ir.ui.view">
    <field name="name">pos.config.form.internal.seq</field>
    <field name="model">pos.config</field>
    <field name="inherit_id" ref="point_of_sale.view_pos_config_form"/>
    <field name="arch" type="xml">
      <xpath expr="//group[@name='receipt']" position="after">
        <group string="Correlativo interno">
          <field name="pos_series_prefix" placeholder="A-"/>
          <field name="pos_internal_sequence_id" readonly="1"/>
        </group>
      </xpath>
    </field>
  </record>
</odoo>
